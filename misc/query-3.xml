<query xmlns='http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd'
       xmlns:foo="http://foo.com/">

   <!-- Query filter example -->

    <and>
        <equal>
            <path>extension</path>
            <value>
                <foo:bar>baz</foo:bar>
            </value>
        </equal>
        <not>
            <true>
                <property>extension/foo:barbar</property>
            </true>
        </not>
        <present>
            <property>extension/foo:x</property>
        </present>
        <or>
            <substring>
                <property>extension/foo:y</property>
                <value>a*b</value>
            </substring>
            <lessOrEqual>
                <property>extension/foo:n</property>
                <value>100</value>
            </lessOrEqual>
            <equal>
                <path>extension</path>
                <value>
                    <foo:foofoo>
                        <foo:foo foo="foo"><foo:foofoo/></foo:foo>
                    </foo:foofoo>
                </value>
            </equal>
        </or>
    </and>
    
</query>
