    <schemaHandling>
        <objectType>
            <kind>account</kind>
            <intent>default</intent>
            <displayName>Default Account</displayName>
            <default>true</default>
            <objectClass>ri:AccountObjectClass</objectClass>

            <attribute>
                <ref>icfs:name</ref>
                <outbound>
                    <source>
                        <path>$user/emailAddress</path>
                    </source>
                    <expression>
                        <script>
                            <code>
                                import com.evolveum.midpoint.util.exception.ExpressionEvaluationException

                                if (emailAddress != null) {
                                    String[] mailArray = emailAddress.toString().split("@")
                                    return mailArray[0] + "@evolveumsandbox.onmicrosoft.com"
                                } else {

                                    throw new ExpressionEvaluationException("Email address can not be null.")
                                }
                            </code>
                        </script>
                    </expression>
                </outbound>
            </attribute>

            <attribute>
                <ref>ri:userPrincipalName</ref>
                <outbound>
                    <source>
                        <path>$user/emailAddress</path>
                    </source>
                    <expression>
                        <script>
                            <code>
                                import com.evolveum.midpoint.util.exception.ExpressionEvaluationException

                                if (emailAddress != null) {
                                    String[] mailArray = emailAddress.toString().split("@")
                                    return mailArray[0] + "@evolveumsandbox.onmicrosoft.com"
                                } else {

                                    throw new ExpressionEvaluationException("Email address can not be null.")
                                }
                            </code>
                        </script>
                    </expression>
                </outbound>
            </attribute>

            <attribute>
                <ref>ri:givenName</ref>
                <outbound>
                    <source>
                        <path>$user/givenName</path>
                    </source>
                </outbound>
            </attribute>

            <attribute>
                <ref>ri:surname</ref>
                <outbound>
                    <source>
                        <path>$user/familyName</path>
                    </source>
                </outbound>
            </attribute>

            <attribute>
                <ref>ri:displayName</ref>
                <outbound>
                    <source>
                        <path>$user/givenName</path>
                    </source>
                    <source>
                        <path>$user/familyName</path>
                    </source>
                    <expression>
                        <script>
                            <code>
                                name = basic.concatName(givenName, familyName)

                                return name
                            </code>
                        </script>
                    </expression>
                </outbound>
            </attribute>

            <attribute>
                <ref>ri:mailNickname</ref>
                <outbound>
                    <source>
                        <path>$user/nickName</path>
                    </source>
                </outbound>
            </attribute>

            <attribute>
