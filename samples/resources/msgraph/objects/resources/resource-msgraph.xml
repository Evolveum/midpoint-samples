                <valueAttribute>ri:id</valueAttribute>
                <shortcutAssociationAttribute>ri:memberOfGroup</shortcutAssociationAttribute>
                <shortcutValueAttribute>icfs:uid</shortcutValueAttribute>
            </association>

            <activation>
