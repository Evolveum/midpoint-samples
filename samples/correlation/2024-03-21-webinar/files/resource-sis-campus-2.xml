<resource xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
          xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
          xmlns:q="http://prism.evolveum.com/xml/ns/public/query-3"
          xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-3"
          oid="851b6547-8288-4f32-96a6-5bb1f2a256fe">
    <name>SIS Campus 2</name>
    <lifecycleState>active</lifecycleState>
    <connectorRef type="ConnectorType">
        <filter>
            <q:text>connectorType = "com.evolveum.polygon.connector.csv.CsvConnector"</q:text>
        </filter>
    </connectorRef>
    <connectorConfiguration xmlns:icfc="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/connector-schema-3">
        <icfc:configurationProperties
                xmlns:gen327="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/bundle/com.evolveum.polygon.connector-csv/com.evolveum.polygon.connector.csv.CsvConnector">
            <gen327:fieldDelimiter>,</gen327:fieldDelimiter>
            <gen327:quote>"</gen327:quote>
            <gen327:headerExists>true</gen327:headerExists>
            <gen327:filePath>/name/of/the/actual/directory/sis-campus-2.csv</gen327:filePath>
            <gen327:nameAttribute>Number</gen327:nameAttribute>
            <gen327:uniqueAttribute>Number</gen327:uniqueAttribute>
        </icfc:configurationProperties>
    </connectorConfiguration>
    <schema>
        <generationConstraints>
            <generateObjectClass>ri:AccountObjectClass</generateObjectClass>
        </generationConstraints>
    </schema>
    <schemaHandling>
        <objectType>
            <kind>account</kind>
            <intent>default</intent>
            <displayName>Default account</displayName>
            <default>true</default>
            <delineation>
                <objectClass>ri:AccountObjectClass</objectClass>
            </delineation>
            <focus>
                <type>c:UserType</type>
                <archetypeRef oid="00000000-0000-0000-0000-000000000702"/>
            </focus>
            <attribute>
                <ref>ri:Number</ref>
                <displayOrder>10</displayOrder>
                <inbound>
                    <name>Personal number</name>
                    <strength>strong</strength>
                    <target>
                        <path>personalNumber</path>
                    </target>
                </inbound>
            </attribute>
            <attribute>
                <ref>ri:GivenName</ref>
                <displayOrder>20</displayOrder>
                <inbound>
                    <name>Given name</name>
                    <strength>strong</strength>
                    <target>
                        <path>givenName</path>
                    </target>
                </inbound>
            </attribute>
            <attribute>
                <ref>ri:MiddleInitial</ref>
                <displayOrder>22</displayOrder>
            </attribute>
            <attribute>
                <ref>ri:Surname</ref>
                <displayOrder>25</displayOrder>
                <inbound>
                    <name>Family name</name>
                    <strength>strong</strength>
                    <target>
                        <path>familyName</path>
                    </target>
                </inbound>
            </attribute>
            <attribute>
                <ref>ri:Title</ref>
                <displayOrder>27</displayOrder>
            </attribute>
            <attribute>
                <ref>ri:Gender</ref>
                <displayOrder>28</displayOrder>
            </attribute>
            <attribute>
                <ref>ri:Birthday</ref>
                <displayOrder>30</displayOrder>
                <inbound>
                    <name>Date of birth</name>
                    <strength>strong</strength>
                    <target>
                        <path>extension/dateOfBirth</path>
                    </target>
                </inbound>
            </attribute>
            <attribute>
                <ref>ri:NationalID</ref>
                <displayOrder>35</displayOrder>
                <inbound>
                    <name>National ID</name>
                    <strength>strong</strength>
                    <target>
                        <path>extension/nationalId</path>
                    </target>
                </inbound>
            </attribute>
            <attribute>
                <ref>ri:StreetAddress</ref>
                <displayOrder>50</displayOrder>
                <inbound>
                    <source>
                        <path>$shadow/attributes/ri:City</path>
                        <name>city</name>
                    </source>
                    <source>
                        <path>$shadow/attributes/ri:StateFull</path>
                        <name>stateFull</name>
                    </source>
                    <source>
                        <path>$shadow/attributes/ri:ZipCode</path>
                        <name>zipCode</name>
                    </source>
                    <source>
                        <path>$shadow/attributes/ri:Country</path>
                        <name>country</name>
                    </source>
                    <expression>
                        <script>
                            <code>"$input, $city, $stateFull, $zipCode, $country"</code>
                        </script>
                    </expression>
                    <target>
                        <path>locality</path>
                    </target>
                    <use>all</use>
                </inbound>
            </attribute>
            <attribute>
                <ref>ri:City</ref>
                <displayOrder>52</displayOrder>
            </attribute>
            <attribute>
                <ref>ri:State</ref>
                <displayOrder>54</displayOrder>
            </attribute>
            <attribute>
                <ref>ri:StateFull</ref>
                <displayOrder>56</displayOrder>
            </attribute>
            <attribute>
                <ref>ri:ZipCode</ref>
                <displayOrder>58</displayOrder>
            </attribute>
            <attribute>
                <ref>ri:Country</ref>
                <displayOrder>60</displayOrder>
            </attribute>
            <synchronization>
                <reaction>
                    <situation>linked</situation>
                    <actions>
                        <synchronize/>
                    </actions>
                </reaction>
                <reaction>
                    <situation>unmatched</situation>
                    <actions>
                        <addFocus/>
                    </actions>
                </reaction>
                <reaction>
                    <situation>unlinked</situation>
                    <actions>
                        <link/>
                    </actions>
                </reaction>
                <reaction>
                    <situation>disputed</situation>
                    <actions>
                        <createCorrelationCase/>
                    </actions>
                </reaction>
            </synchronization>
        </objectType>
    </schemaHandling>
</resource>
