<role oid="1ba23cd0-8fe4-11e2-86e1-3c970e467874" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-2a"
