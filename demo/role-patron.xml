	xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-2">
    <name>Patron</name>    
    <description>A role for art patrons. Designed to be assigned manually.</description>
    <assignment>
	<description>Addressbook resource</description>
    	<accountConstruction>
