<!--

This file is an example of Resource definition. It defines an LDAP resource
using an Indentity Connector Framework LDAP connector. It contains proper schema
for use with a stock OpenDJ and OpenDS servers.

This resource definition contains also definition to enable synchronization.

-->
<objects xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2"
	xmlns:idfs="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/resource-schema-1.xsd"
	xmlns:my="http://myself.me/schemas/whatever"
	xsi:schemaLocation="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd ../../infra/schema/src/main/resources/xml/ns/public/common/common-1.xsd">


	<resource oid="ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2">
    
    	<!-- Resource name. It will be displayed in GUI.  -->
		<name>Localhost OpenDJ</name>
     
     	<!-- Reference to the ICF LDAP connector. OID is "virtual" for now. -->
		<connectorRef type="ConnectorType">
			<and>
		        <type uri="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd#ConnectorType"/>
		        <equal>
		          <value>
		              <connectorType>org.identityconnectors.ldap.LdapConnector</connectorType>
		          </value>
		        </equal>
		      </and>
		</connectorRef>
        <!-- Resource namespace. This is important. All resource schema elements should be in this namespace. -->
		<namespace>http://midpoint.evolveum.com/xml/ns/public/resource/instance/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2</namespace>

        <!-- Configuration section contains configuration of the connector,
             such as hostnames and passwords -->
		<configuration
			xmlns:icfcldap="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector"
	               xmlns:icfc="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/configuration-1.xsd">
                <!-- Propeties of ICF connector bundle. The applicable element
                     names can be found in the ICF connector documentation -->
            <!-- Configuration specific for the LDAP connector -->
			<icfcldap:configurationProperties>
				<icfcldap:port>1389</icfcldap:port>
				<icfcldap:host>localhost</icfcldap:host>
				<icfcldap:baseContexts>dc=example,dc=com</icfcldap:baseContexts>
				<icfcldap:principal>uid=idm,ou=Administrators,dc=example,dc=com</icfcldap:principal>
				<icfcldap:credentials>secret</icfcldap:credentials>
				<icfcldap:modifiersNamesToFilterOut>uid=idm,ou=Administrators,dc=example,dc=com</icfcldap:modifiersNamesToFilterOut>
				<icfcldap:vlvSortAttribute>uid</icfcldap:vlvSortAttribute>
				<icfcldap:usePagedResultControl>true</icfcldap:usePagedResultControl>
                    		<icfcldap:accountObjectClasses>top</icfcldap:accountObjectClasses>
		                <icfcldap:accountObjectClasses>person</icfcldap:accountObjectClasses>
		                <icfcldap:accountObjectClasses>organizationalPerson</icfcldap:accountObjectClasses>
		                <icfcldap:accountObjectClasses>inetOrgPerson</icfcldap:accountObjectClasses>
               			<icfcldap:accountObjectClasses>piracyPerson</icfcldap:accountObjectClasses>
			</icfcldap:configurationProperties>
		
        </configuration>

        <!-- Resource Schema Handling definition.
             This part defines how the schema defined above will be used by
             OpenIDM. It defines expressions and limitations for individual
             schema attributes.

             The expressions that describe both inbound and outbound flow of
             the attributes are defined in this section.

             This is the part where most of the customization takes place.
        -->
        <schemaHandling>
            <!-- Definition of default account type.
                 This is now the only account type that OpenIDM can work with. -->
            <accountType default="true">
                <!-- Readable name for the account type -->
                <name>Default Account</name>
                <!-- Reference to the Resoure Schema (see above) specifying
                     object class for this account type -->
                <objectClass>rAccount</objectClass>
                <!-- Definition of __NAME__ attribute handling.
                     ICF will put entry DN into __NAME__ attribute.-->
                <attribute ref="idfs:name">
                    <!-- Readable name for the attribute. This will be used in GUI -->
                    <name>Distinguished Name</name>
                    <!-- Access limitation for this attribute.
                         In our case ICF will put DN in the __NAME__ attribute.
                         Althouhg DN can be theoretically changes, this definition
                         will make it immutable. -->
                    <access>create</access>
                    <access>read</access>
                    <!-- Outbound expression for the __NAME__ attribute.
                         If account is created or updated, the __NAME__ attribute
                         will be set accrding to this expression.

                         This specific expression will construct DN by concatenating
                         name from user object and string constants.

                         The expression is marked as "default", therefore it will
                         be evaluated only if the entry already does not have an DN.

                         It is an XPath expression, similar to BPEL assignment expressions. -->
                    <outbound default="true">
                        <valueExpression>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            concat('uid=', $user/name, ',ou=people,dc=example,dc=com')
                            </valueExpression>
                    </outbound>
                    <!-- No inbound expression here. We cannot sync changes in DN yet -->
                </attribute>
                <!-- Definition of __UID__ attribute handling.
                     ICF will put entry UUID into __UID__ attribute in case it
                     is connecting to OpenDJ/OpenDS. It may be different for
                     other directory servers and it also dependes on connector
                     configuration. Please see connector documentation for
                     more details.-->
                <attribute ref="idfs:uid">
                    <!-- Readable name for the attribute. This will be used in GUI -->
                    <name>Entry UUID</name>
                    <!-- Access limitation for this attribute.
                         Entry UUID is created by the directory server itelf
                         and it is immutable. We can only read it. -->
                    <access>read</access>
                    <!-- There is no "outbound" expression, so OpenIDM will not
                         do anything with this attribute. -->
                    <!-- No inbound expression here. Does not make sense to sync UUID as it is supposed to be immutable -->
                </attribute>
                <!-- Definition of cn attribute handling. -->
                <attribute ref="rcn">
                    <name>Common Name</name>
                    <access>create</access>
                    <access>read</access>
                    <access>update</access>
                    <!-- Outbound expression here is using "pure" XPath, without
                         using functions. This is the simplest case, just copying
                         the "fullName" property of user to the "cn" attribute of
                         an account.

                         Please note that this expression is not marked as "default".
                         Therefore this value will be copied on every change,
                         overwriting the original value of "cn" on the resource.
                     -->
                    <outbound>
                        <valueExpression>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            $user/fullName
                        </valueExpression>
                    </outbound>
                    <!-- Inbound expression. It is used to synchronize changes
                         in the account to the user object in the IDM. This
                         expression specifies to copy the cn attribute value
                         from the account to the fullName property of the user.
                     -->
                    <inbound>
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            $user/fullName
                        </target>
                    </inbound>
                </attribute>
                <!-- Definition of sn attribute handling. -->
                <attribute ref="rsn">
                    <name>Surname</name>
                    <access>create</access>
                    <access>read</access>
                    <access>update</access>
                    <outbound>
                        <valueExpression>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            $user/familyName
                        </valueExpression>
                    </outbound>
                    <!-- sn will be synchronized to user's familyName -->
                    <inbound>
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            $user/familyName
                        </target>
                    </inbound>
                </attribute>
                <!-- Definition of givenName attribute handling. -->
                <attribute ref="rgivenName">
                    <name>Given Name</name>
                    <access>create</access>
                    <access>read</access>
                    <access>update</access>
                    <outbound>
                        <valueExpression>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            $user/givenName
                        </valueExpression>
                    </outbound>
                    <!-- givenName will be synchronized to user's givenName -->
                    <inbound>
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            $user/givenName
                        </target>
                    </inbound>
                </attribute>
                <!-- Definition of uid attribute handling.
                     Do not confuse with __UID__! -->
                <attribute ref="ruid">
                    <name>Login Name</name>
                    <access>create</access>
                    <access>read</access>
                    <outbound>
                        <!-- It is mapped from (and also to) "name" property of
                             user. It is essentially a login name -->
                        <valueExpression>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            $user/name
                        </valueExpression>
                    </outbound>
                    <!-- It is mapped to (and also from) "name" property of
                             user. It is essentially a login name -->
                    <inbound>
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            $user/name
                        </target>
                    </inbound>
                </attribute>
                <!-- Definition of description attribute handling. -->
                <attribute ref="rdescription">
                    <!-- This outbound expression is not really an expression.
                         It is using a fixed literal insead of dynamically
                         generated value. -->
                    <outbound default="true">
                        <value>Created by IDM</value>
                    </outbound>
                    <!-- No outbound expression for description. We don't want to synchronize that -->
                </attribute>
                <attribute xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" ref="rpiracyShipName">
                    <inbound xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector/1.0.5531" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" xmlns:idc="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                            $user/extension/rpiracyShipName
                        </target>
                    </inbound>
                </attribute>
                <attribute xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" ref="rpiracyStatus">
                    <inbound xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector/1.0.5531" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" xmlns:idc="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                            $user/extension/rpiracyStatus
                        </target>
                    </inbound>
                </attribute>
                <attribute xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" ref="rpiracyScarNumber">
                    <inbound xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector/1.0.5531" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" xmlns:idc="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                            $user/extension/rpiracyScarNumber
                        </target>
                    </inbound>
                </attribute>
                <attribute xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" ref="rpiracyCaptainship">
                    <inbound xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector/1.0.5531" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" xmlns:idc="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                            $user/extension/rpiracyCaptainship
                        </target>
                    </inbound>
                </attribute>
                <attribute xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" ref="rpiracyEnrollmentTimestamp">
                    <inbound xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector/1.0.5531" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" xmlns:idc="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                            $user/extension/rpiracyEnrollmentTimestamp
                        </target>
                    </inbound>
                </attribute>
                <attribute xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" ref="rpiracyTreasureSecret">
                    <inbound xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector/1.0.5531" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" xmlns:idc="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                            $user/extension/rpiracyTreasureSecret
                        </target>
                    </inbound>
                </attribute>
                <attribute xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" ref="rpiracyNickname">
                    <inbound xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector/1.0.5531" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" xmlns:idc="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                            $user/extension/rpiracyNickname
                        </target>
                    </inbound>
                </attribute>
                <attribute xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" ref="rpiracyBestWeapon">
                    <inbound xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector/1.0.5531" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" xmlns:idc="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                            $user/extension/rpiracyBestWeapon
                        </target>
                    </inbound>
                </attribute>
                <attribute xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" ref="rpiracyQuote">
                    <inbound xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector/1.0.5531" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" xmlns:idc="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                            $user/extension/rpiracyQuote
                        </target>
                    </inbound>
                </attribute>
                <attribute xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" ref="rpiracyDrink">
                    <inbound xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.ldap/org.identityconnectors.ldap.LdapConnector/1.0.5531" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" xmlns:idc="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
                        <target>
                            declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                            declare namespace ri="http://midpoint.evolveum.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                            $user/extension/rpiracyDrink
                        </target>
                    </inbound>
                </attribute>
            </accountType>
        </schemaHandling>
        <!--
            Synchronization section describes the synchronization policy, timing,
            reactions and similar synchronization settings.
        -->
        <synchronization>
            <!--
                The synchronization for this resource is enabled.
                It means that the synchronization will poll for changes once
                per interval specified below. -->
            <enabled>true</enabled>
            <!-- Synchronization polling interval, in seconds -->
            <pollingInterval>60</pollingInterval>
            <!-- Correlation expression.
                 It will be used to find appropriate user entry for an account.
              -->
            <correlation>
                <!-- Correlation rule is a search query -->
                <!-- The clause <type uri="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd#UserType"/> is implicit in correlation rules -->
                <!-- Following search queury will look for users that have "name"
                     equal to the "uid" attribute of the account. Simply speaking,
                     it will look for match in usernames in the IDM and the resource. -->
                <equal>
                    <path>.</path>
                    <valueExpression>
			<ref>name</ref>
			<code>
                        declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                        declare namespace dj="http://midpoint.evolveum.com/xml/ns/public/resource/instance/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2";
                        $account/attributes/dj:uid
			</code>
                    </valueExpression>
                </equal>
            </correlation>
            <!-- Confirmation rule may be here, but as the search above will
                 always return at most one match, the confirmation rule is not needed. -->
            <!-- Following section describes reactions to a situations.
                 The setting here assumes that this resource is authoritative,
                 therefore all accounts created on the resource should be
                 reflected as new users in IDM.
                 See https://wikis.forgerock.org/confluence/display/openidm/Synchronization+Situations
             -->
            <reaction>
                <situation>confirmed</situation>
                <action ref="http://midpoint.evolveum.com/xml/ns/public/model/action-1#modifyUser"/>
            </reaction>
            <reaction>
                <situation>deleted</situation>
                <action ref="http://midpoint.evolveum.com/xml/ns/public/model/action-1#unlinkAccount"/>
            </reaction>
            <reaction>
                <situation>missing</situation>
                <action ref="http://midpoint.evolveum.com/xml/ns/public/model/action-1#unlinkAccount"/>
            </reaction>
            <reaction>
                <situation>found</situation>
                <action ref="http://midpoint.evolveum.com/xml/ns/public/model/action-1#linkAccount"/>
            </reaction>
            <reaction>
                <situation>unassigned</situation>
                <action ref="http://midpoint.evolveum.com/xml/ns/public/model/action-1#linkAccount"/>
            </reaction>
            <reaction>
                <situation>unmatched</situation>
                <action ref="http://midpoint.evolveum.com/xml/ns/public/model/action-1#addUser">
                    <!-- Reference to the User Template is here. If the user would be
                         created as a result of this action, it will be created according
                         to this template. -->
                    <userTemplateRef oid="c0c010c0-d34d-b33f-f00d-777111111111"/>
                </action>
                <action ref="http://midpoint.evolveum.com/xml/ns/public/model/action-1#linkAccount"/>
            </reaction>
        </synchronization>
    </resource>
    <!-- User Template Object.
          This object is used when creating a new account, to set it up as needed.
     -->
    <userTemplate oid="c0c010c0-d34d-b33f-f00d-777111111111">
        <name>Default User Template</name>
        <!-- Property construction.
             Defines how properties of user object are set up.
             This specific definition sets a full name as a concatenation
             of givenName and familyName. -->
        <propertyConstruction>
            <property>fullName</property>
            <valueConstruction default="true">
                <valueExpression>
                    declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd";
                    concat($user/givenName,' ',$user/familyName)
                </valueExpression>
            </valueConstruction>
        </propertyConstruction>
        <!-- Account construction.
             Defines what accounts should be assigned to a user when it is
             created. It only defines resource and account type. The rest of the
             account parameters are specified by outbound expressions in the
             account type in the resource definition, as usual.
             See https://wikis.forgerock.org/confluence/display/openidm/Synchronization
             -->
        <accountConstruction>
            <resourceRef oid="c0c010c0-d34d-b33f-f00d-333111111112" type="ResourceType"/>
            <type>default</type>
        </accountConstruction>
    </userTemplate>
</objects>
